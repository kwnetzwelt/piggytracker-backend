{
  "name": "piggytracker-api",
  "version": "1.0.0",
  "description": "API backend for https://piggytracker.org",
  "main": "index.js",
  "scripts": {
    "eslint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "start": "node dist/index.js",
    "compile": "ts-node build.ts && tsc",
    "dev": "nodemon server/index.ts | pino-pretty",
    "dev:debug": "nodemon --exec 'node -r ts-node/register --inspect-brk' server/index.ts | pino-pretty",
    "test": "mocha test/**/*.ts --exit",
    "test:coverage": "nyc mocha test/**/*.ts --exit",
    "test:debug": "mocha --inspect-brk test/**/*.ts --exit",
    "cli": "ts-node cli/cli.ts"
  },
  "author": "",
  "dependencies": {
    "bluebird": "^3.5.3",
    "body-parser": "^1.18.3",
    "commander": "^5.0.0",
    "cookie-parser": "^1.4.3",
    "dotenv": "^6.1.0",
    "express": "^4.16.4",
    "http-status-codes": "^1.3.0",
    "jsonwebtoken": "^8.5.1",
    "mongoose": "^5.9.10",
    "passport": "^0.4.1",
    "passport-google-oauth": "^2.0.0",
    "passport-jwt": "^4.0.0",
    "pino": "^5.8.1",
    "readline-sync": "^1.4.10",
    "swagger-express-middleware": "^1.2.0"
  },
  "devDependencies": {
    "@istanbuljs/nyc-config-typescript": "^1.0.1",
    "@types/bluebird": "^3.5.24",
    "@types/chai": "^4.1.4",
    "@types/express": "^4.16.0",
    "@types/mocha": "^5.2.5",
    "@types/mongoose": "^5.3.1",
    "@types/passport": "^1.0.3",
    "@types/passport-jwt": "^3.0.3",
    "@types/pino": "^5.20.0",
    "@types/supertest": "^2.0.8",
    "@typescript-eslint/eslint-plugin": "^2.29.0",
    "@typescript-eslint/parser": "^2.29.0",
    "chai": "^4.1.2",
    "eslint": "^6.8.0",
    "eslint-config-airbnb-typescript": "^7.2.1",
    "eslint-plugin-import": "^2.20.2",
    "mocha": "^5.2.0",
    "nodemon": "^1.18.6",
    "nyc": "^15.0.1",
    "pino-pretty": "^2.2.3",
    "shelljs": "^0.8.3",
    "source-map-support": "^0.5.19",
    "supertest": "^3.3.0",
    "ts-node": "^7.0.1",
    "tslint": "^5.11.0",
    "typescript": "^3.1.6"
  }
